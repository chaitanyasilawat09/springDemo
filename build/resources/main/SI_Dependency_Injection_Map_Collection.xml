<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "https://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
    <bean id="mapBean" class="SampleBeanMap">
        <property name="data">
            <list>
                <value>abc</value>
                <value>chait</value>
                <value>Slaw</value>
            </list>
        </property>
        <property name="employee">
            <map>
                <entry key="e1" value="10000"/>
                <entry key="e2" value="20000"/>
                <entry key="e3" value="30000"/>
            </map>
        </property>
    </bean>
</beans>